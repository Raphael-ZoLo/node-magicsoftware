{
	"compilerOptions": {
        "target": "es5",
        "module": "commonjs",
		"sourceMap": true,
		"watch": true,
        "isolatedModules": false,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": true,
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true	
	}
}